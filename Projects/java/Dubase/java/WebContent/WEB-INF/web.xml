<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Dubase</display-name>
  <welcome-file-list>
    <welcome-file>index.xhtml</welcome-file>
  </welcome-file-list>
  <env-entry>
    <env-entry-name>es.ehubio.dubase.inputDir</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>/media/data/Sequences/Dubase</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>es.ehubio.dubase.tmpDir</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>/tmp</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>es.ehubio.dubase.rcptTo</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>juanmanuel.ramirez@ehu.eus</env-entry-value>
  </env-entry>
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>*.xhtml</url-pattern>
  </servlet-mapping>
</web-app>