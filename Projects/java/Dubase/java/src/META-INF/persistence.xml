		<class>es.ehubio.dubase.dl.entities.Clazz</class>
		<class>es.ehubio.dubase.dl.entities.Condition</class>
		<class>es.ehubio.dubase.dl.entities.Enzyme</class>
		<class>es.ehubio.dubase.dl.entities.EvScore</class>
		<class>es.ehubio.dubase.dl.entities.Evidence</class>
		<class>es.ehubio.dubase.dl.entities.Experiment</class>
		<class>es.ehubio.dubase.dl.entities.FileType</class>
		<class>es.ehubio.dubase.dl.entities.Gene</class>
