<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="Dubase" transaction-type="JTA">
		<jta-data-source>java:jboss/datasources/Dubase</jta-data-source>
		<class>es.ehubio.dubase.dl.Ambiguity</class>
		<class>es.ehubio.dubase.dl.Author</class>
		<class>es.ehubio.dubase.dl.Clazz</class>
		<class>es.ehubio.dubase.dl.Enzyme</class>
		<class>es.ehubio.dubase.dl.EvScore</class>
		<class>es.ehubio.dubase.dl.Evidence</class>
		<class>es.ehubio.dubase.dl.Experiment</class>
		<class>es.ehubio.dubase.dl.Method</class>
		<class>es.ehubio.dubase.dl.RepScore</class>
		<class>es.ehubio.dubase.dl.Replicate</class>
		<class>es.ehubio.dubase.dl.ScoreType</class>
		<class>es.ehubio.dubase.dl.Substrate</class>
		<class>es.ehubio.dubase.dl.Superfamily</class>
      	<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect"/>
			<property name="hibernate.hbm2ddl.auto" value="validate"/>
		</properties>
	</persistence-unit>
</persistence>
