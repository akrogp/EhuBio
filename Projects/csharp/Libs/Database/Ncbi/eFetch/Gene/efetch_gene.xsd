            </xs:simpleType>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
<!-- restriction site  (for maps really) -->
      <xs:element name="SeqFeatData_rsite">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:Rsite-ref"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- user defined structure -->
      <xs:element name="SeqFeatData_user">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:User-object"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- transcription initiation -->
      <xs:element name="SeqFeatData_txinit">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:Txinit"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
<!-- a numbering system -->
      <xs:element name="SeqFeatData_num">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:Numbering"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="SeqFeatData_psec-str">
        <xs:complexType>
