      <xs:element name="Seq-id_embl">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:Textseq-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Seq-id_pir">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:Textseq-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Seq-id_swissprot">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:Textseq-id"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Seq-id_patent">
