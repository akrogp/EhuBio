          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Org-ref_syn_E" type="xs:string"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Org-ref_orgname" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
