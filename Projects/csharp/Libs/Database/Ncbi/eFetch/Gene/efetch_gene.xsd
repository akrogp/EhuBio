      <xs:element name="Id-pat_doc-type" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<!-- letter, thesis, or manuscript -->
<xs:element name="Cit-let">
  <xs:complexType>
    <xs:sequence>
<!-- same fields as a book -->
      <xs:element name="Cit-let_cit">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="tns:Cit-book"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
